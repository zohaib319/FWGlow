<?xml version="1.0" encoding="utf-8"?>
<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapsActivity5">
<androidx.constraintlayout.widget.ConstraintLayout
android:layout_width="match_parent"
android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="505dp"
        android:background="@drawable/bg_4"
        map:layout_constraintBottom_toBottomOf="parent"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.0"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toTopOf="@+id/imageView2"
        map:layout_constraintVertical_bias="1.0" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Timer"
        android:textColor="#ffffff"
        android:textSize="30dp"
        map:layout_constraintBottom_toTopOf="@+id/chrom"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.502"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/imageView2"
        map:layout_constraintVertical_bias="1.0" />

    <Chronometer
        android:id="@+id/chrom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="#ffffff"
        map:layout_constraintBottom_toTopOf="@+id/btn_cancel"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.501"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/imageView2"
        map:layout_constraintVertical_bias="0.514" />

    <View
    android:id="@+id/button2"
    android:layout_width="292dp"
    android:layout_height="73dp"
    android:background="@drawable/home_front"
    android:drawableRight="@drawable/arrow2"
    android:paddingRight="50dp"
    android:text="User Name"
    map:layout_constraintBottom_toBottomOf="parent"
    map:layout_constraintEnd_toEndOf="parent"
    map:layout_constraintHorizontal_bias="0.495"
    map:layout_constraintStart_toStartOf="parent"
    map:layout_constraintTop_toTopOf="parent"
    map:layout_constraintVertical_bias="0.094" />

    <TextView
        android:id="@+id/textView75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="160dp"
        android:layout_marginRight="160dp"
        android:text="Service name"
        android:textSize="10dp"
        map:layout_constraintBottom_toBottomOf="@+id/button2"
        map:layout_constraintEnd_toEndOf="@+id/button2"
        map:layout_constraintHorizontal_bias="0.0"
        map:layout_constraintStart_toEndOf="@+id/imageView12"
        map:layout_constraintTop_toTopOf="@+id/button2"
        map:layout_constraintVertical_bias="0.288" />

    <TextView
        android:id="@+id/textView76"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Classic Manicure"
        android:textSize="15dp"
        map:layout_constraintBottom_toBottomOf="@+id/button2"
        map:layout_constraintEnd_toEndOf="@+id/button2"
        map:layout_constraintHorizontal_bias="0.0"
        map:layout_constraintStart_toEndOf="@+id/imageView13"
        map:layout_constraintTop_toBottomOf="@+id/textView75"
        map:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ITEM LIST"
        android:textSize="15dp"
        map:layout_constraintBottom_toBottomOf="@+id/button2"
        map:layout_constraintEnd_toEndOf="@+id/button2"
        map:layout_constraintHorizontal_bias="0.692"
        map:layout_constraintStart_toEndOf="@+id/textView75"
        map:layout_constraintTop_toTopOf="@+id/button2"
        map:layout_constraintVertical_bias="0.326" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/dp_box"
        map:layout_constraintBottom_toBottomOf="@+id/button2"
        map:layout_constraintEnd_toEndOf="@+id/button2"
        map:layout_constraintHorizontal_bias="0.078"
        map:layout_constraintStart_toStartOf="@+id/button2"
        map:layout_constraintTop_toTopOf="@+id/button2"
        map:layout_constraintVertical_bias="0.347" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="157dp"
        android:layout_height="159dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/dp_box"
        map:layout_constraintBottom_toTopOf="@+id/textView30"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintHorizontal_bias="0.501"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:layout_width="380dp"
        android:layout_height="58dp"
        android:background="@drawable/button_bg"
        android:onClick="startChronometer"
        android:paddingLeft="120dp"
        android:paddingRight="120dp"
        android:text="start Timer"
        android:textColor="#ffffff"
        map:layout_constraintBottom_toBottomOf="@+id/imageView"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/button2"
        map:layout_constraintVertical_bias="0.848" />

    <Button
        android:id="@+id/btn_cancel"
        android:layout_width="380dp"
        android:layout_height="58dp"
        android:background="@drawable/button_bg"
        android:paddingLeft="120dp"
        android:paddingRight="120dp"
        android:text="Cancel"
        android:textColor="#ffffff"
        map:layout_constraintBottom_toBottomOf="@+id/imageView"
        map:layout_constraintEnd_toEndOf="parent"
        map:layout_constraintStart_toStartOf="parent"
        map:layout_constraintTop_toBottomOf="@+id/button2"
        map:layout_constraintVertical_bias="0.959" />


</androidx.constraintlayout.widget.ConstraintLayout>
</fragment>